<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yszln.qiuqiu.mapper.MessageMapper">
    <select id="findAll" resultType="com.yszln.qiuqiu.entity.Message">
        select * from  tb_message
    </select>
    <insert id="insert" parameterType="com.yszln.qiuqiu.entity.Message">
         insert into tb_message (content,url,`type`,sourceId,receiveId) values (#{content},#{url},#{type},#{sourceId},#{receiveId})
    </insert>
</mapper>